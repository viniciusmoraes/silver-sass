// variables.json
{
	"articleRadius": {
		"selector": "article",
		"hash": "tag", //Hash: id, class or tag
		"radius": "20%"
	},

	"messageFlash": {
		"selector": "h1",
		"hash": "tag", //Hash: id, class or tag
		"name": "blinker",
		"duration": "1.2s",
		"timing": "linear",
		"iteration": "infinite"
	},


	"articleBoxShadow": {
		"selector": "wrapper",
		"hash": "class",
		"inset": "no", //no, inset
		"hLength": "0",
		"vLength": "0",
		"blurRadius": "10px",
		"spread": "10px",
		"colorType": "hex",
		"colorValue": "#00FF00"
	},

	"h2TextShadow": {
		"selector": "h2",
		"hash": "tag",
		"hLength": "0",
		"vLength": "0",
		"blurRadius": "10px",
		"colorType": "hex",
		"colorValue": "#00FF00"
	}

}